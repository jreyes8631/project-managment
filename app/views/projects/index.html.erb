<div class="card">
  <div class="card-body">
    <h2 class="card-title d-flex justify-content-between mb-4">
      <span>Projects</span>
      <%= link_to 'New Project', new_project_path, class: 'btn btn-primary' %> </h2>
    
      <% @projects.each do |project| %>
        
          <div class="card">
            <div class="card-body">
                <h5 class='card-title'><%= link_to project.name, project_path(project) %></h5>
            </div>
          </div>
        
      <%end%>
  </div>
</div>



